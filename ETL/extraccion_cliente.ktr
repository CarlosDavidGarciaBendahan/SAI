    <connection>prueba</connection>
    <sql>SELECT
  cl.id
, cl.cli_jur_nombre
, cl.cli_jur_direccion
, cl.cli_jur_identificador
, cl.cli_jur_rif
, (select par_nombre from parroquia where id = cl.cli_jur_fk_parroquia) as parroquia
, (select mun_nombre from municipio 
	where id =
(select par_fk_municipio from parroquia where id = cl.cli_jur_fk_parroquia)) as municipio
, (select est_nombre from estado 
	where id = 
	(select mun_fk_estado from municipio 
		where id =
		(select par_fk_municipio from parroquia where id = cl.cli_jur_fk_parroquia)
	)
 ) as estado
, cl.created_at
, cl.updated_at
,(select ven_fecha_compra from venta
	where ven_eliminada = 0 and ven_fk_cliente_juridico = cl.id
	order by ven_fecha_compra
	limit 1) as fecha_primera_compra
FROM cliente_juridico as cl
</sql>
