    <connection>prueba</connection>
    <sql>SELECT
  id
, cantidad_faltante
, cotizacion_dolar
, fk_presupuesto
, (select pro_com_codigo from producto_computador where id = fk_producto_computador) as codigo_general_computador
, (select mar_marca from marca 
	where id =(select mod_fk_marca from modelo 
				where id =(select pro_com_fk_modelo from producto_computador 
							where id = fk_producto_computador)
			  ) 
  ) as marca_computador
, (select mod_modelo from modelo 
				where id =(select pro_com_fk_modelo from producto_computador 
							where id = fk_producto_computador)
			  ) 
   as modelo_computador
, (select tip_tipo from tipo_producto 
				where id =(select pro_com_fk_tipo_producto from producto_computador 
							where id = fk_producto_computador)
 ) 
   as tipo_computador
, (select pro_art_codigo from producto_articulo   where id = fk_producto_articulo) as codigo_general_articulo
, (select mar_marca from marca 
	where id =(select mod_fk_marca from modelo 
				where id =(select pro_art_fk_modelo from producto_articulo 
							where id = fk_producto_articulo)
			  ) 
  ) as marca_articulo
, (select mod_modelo from modelo 
				where id =(select pro_art_fk_modelo from producto_articulo 
							where id = fk_producto_articulo)
			  ) 
   as modelo_articulo
, (select tip_tipo from tipo_producto 
				where id =(select pro_art_fk_tipo_producto from producto_articulo 
							where id = fk_producto_articulo)
 ) 
   as tipo_articulo
, (select pro_art_capacidad from producto_articulo   where id = fk_producto_articulo) as capacidad_articulo
, (select uni_medida from unidadmedida where id =(select pro_art_fk_unidadmedida from producto_articulo 
							where id = fk_producto_articulo)) as unidad_medida_articulo
, precio_unitario
, fk_producto_computador
, fk_producto_articulo
FROM historico_falta_stock
</sql>
