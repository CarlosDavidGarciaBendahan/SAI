    <connection>prueba</connection>
    <sql>SELECT

  v.id as ven_id, v.ven_fecha_compra, v.ven_monto_total, v.ven_moneda, v.ven_porcentaje_descuento, v.ven_eliminada
, (select cli_nat_identificador||'-'||cli_nat_cedula from cliente_natural where id = v.ven_fk_cliente_natural) as cedula
, (select cli_jur_identificador||'-'||cli_jur_rif from cliente_juridico where id = v.ven_fk_cliente_juridico) as rif
, v.ven_fk_cliente_juridico , v.ven_fk_cliente_natural, v.ven_fk_fuenteventa, v.ven_fk_presupuesto
, cpc.cod_pc_codigo as codigo_producto, cpc.id as id_producto, pcv.precio_unitario, cpc.cod_pc_costo as costo_producto


FROM venta v, pc_venta pcv, codigopc cpc
WHERE v.id = pcv.pc_ven_fk_venta and cpc.id = pcv.pc_ven_fk_codigopc

UNION

SELECT

  v.id as ven_id, v.ven_fecha_compra, v.ven_monto_total, v.ven_moneda, v.ven_porcentaje_descuento, v.ven_eliminada
, (select cli_nat_identificador||'-'||cli_nat_cedula from cliente_natural where id = v.ven_fk_cliente_natural) as cedula
, (select cli_jur_identificador||'-'||cli_jur_rif from cliente_juridico where id = v.ven_fk_cliente_juridico) as rif
, v.ven_fk_cliente_juridico , v.ven_fk_cliente_natural, v.ven_fk_fuenteventa, v.ven_fk_presupuesto
, ca.cod_art_codigo as codigo_producto, ca.id as id_producto, av.precio_unitario, ca.cod_art_costo as costo_producto


FROM venta v, articulo_venta av, codigoarticulo ca
WHERE v.id = av.art_ven_fk_venta and ca.id = av.art_ven_fk_codigoarticulo


</sql>
